#Equipos participantes
equipolocal = 'Cali'
equipovisitante = 'America de Cali'
equipovisitante2 = 'America de Cali2'

#Marcador inicial    
goleslocal = 1
golesvisitante = 0

#Marcador primer tiempo
goleslocal+=2
golesvisitante+=2
golesvisitante-=1

#Marcador segundo tiempo
goleslocal+=1
golesvisitante+=1


       
print(equipolocal,' ',goleslocal,' - ',equipovisitante,' ',golesvisitante,"Prueba",equipovisitante2 )
